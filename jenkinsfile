pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload_Loans')
        {
            steps
            {
             git 'https://github.com/Tamaraniprasad/mavenabc.git'
            }
        }
        stage('ContinuousBuild_Loans')
        {
            steps
            {
             sh 'mvn package'
            }
        }
     } 
} 
   
