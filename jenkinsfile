@Library('mylibraries')_
pipeline
{
    agent any
    stages
    {
         stage('Continuous Download Loans')
           {
               
              steps
              {
                
               script
                  {
                     cicd.newGit("https://github.com/Tamaraniprasad/mavenabc.git")
                   }
               }
            }
            stage('Continuous Build Loans') 
            {
                steps
                {
                    script
                    {
                        cicd.newMaven()
                    }
                }
            }
     }
}
